<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - JobPortal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">JobPortal</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>


                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#">
                                Admin
                            </a>
                            <div class="dropdown-menu">
                                        <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="UserTypes" asp-action="Index">User Type</a>
                              
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="AccountStatus" asp-action="Index">Account Status</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="UserTables" asp-action="Index">User Table</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="CurrentJobStatus" asp-action="Index">Cuurent Job Status</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="JobCategories" asp-action="Index">Job Category</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Countries" asp-action="Index">Country</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Employees" asp-action="Index">Employee</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Certificates" asp-action="Index">Certificate</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Languages" asp-action="Index">Language</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Educations" asp-action="Index">Education</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Skills" asp-action="Index">Skill</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="WorkExperiences" asp-action="Index">Work Experience</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="JobApplyStatus" asp-action="Index">Job Apply Status</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="JobApplies" asp-action="Index">Job Apply</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Jobs" asp-action="Index">Job</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="Companies" asp-action="Index">Company</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="JobStatus" asp-action="Index">Job Status</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="PostJobs" asp-action="Index">Post Job</a>
                                <a class="dropdown-item"
                                   asp-area="PortalMgmt" asp-controller="EventTables" asp-action="Index">Event Table</a>


                            </div>
                        </li>

                    </ul>
                </div>
                <partial name="_LoginPartial" />
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - JobPortal - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
